<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/juti_fps_lay"
            android:padding="10dp"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/pungjunzhenlv"
                android:textColor="@color/black"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/juti_fps_liuchang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:background="@null"
                android:text="29.7fps" />
        </RelativeLayout>
        <LinearLayout

            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"/>

        <RelativeLayout
            android:id="@+id/juti_fangcha_lay"
            android:padding="10dp"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/fangcha"
                android:textColor="@color/black"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/juti_fangcha_liuchang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.06"
                android:layout_centerVertical="true"
                android:background="@null"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <LinearLayout

            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"/>
        <RelativeLayout
            android:id="@+id/juti_dizhenlv_lay"
            android:padding="10dp"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="低帧率(%)"
                android:textColor="@color/black"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/juti_dizhenlv_liuchang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="6.63%"
                android:layout_centerVertical="true"
                android:background="@null"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <LinearLayout

            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"/>
        <RelativeLayout
            android:id="@+id/juti_zhenjiange_lay"
            android:padding="10dp"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/zhenjiange"
                android:textColor="@color/black"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/juti_zhenjiange_liuchang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="22.93ms"
                android:layout_centerVertical="true"
                android:background="@null"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <LinearLayout

            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"/>
        <RelativeLayout
            android:id="@+id/juti_jank_lay"
            android:padding="10dp"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="jank(卡顿次数/10min)"
                android:textColor="@color/black"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/juti_jank_liuchang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.18"
                android:layout_centerVertical="true"
                android:background="@null"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <LinearLayout

            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"/>
        <RelativeLayout
            android:id="@+id/juti_kadun_lay"
            android:padding="10dp"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:text="@string/kadunshichangzhanbi"
                android:textColor="@color/black"
                android:textSize="16sp" />
            <TextView
                android:id="@+id/juti_kadun_liuchang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0.76%"
                android:layout_centerVertical="true"
                android:background="@null"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <LinearLayout

            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"/>

    </LinearLayout>
    </ScrollView>

</LinearLayout>